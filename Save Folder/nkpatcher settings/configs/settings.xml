					<Item Action="Unrar" arg1="E:\UDATA\21585554\000000000000\softmod files\files.rar" arg2="Y:\"/>
					<Item Action="Unrar" arg1="Y:\nkpatcher.rar" arg2="Y:\"/>
					<Item Action="Copy" arg1="Y:\nkpatcher\configs\dualboot\dualboot.settings" arg2="C:\nkpatcher\configs\dualboot\dualboot.settings"/>
					<Item Action="Format" arg1="Y"/>
					<Item Action="MessageBox" Arg1="Complete">Dualboot.settings has been restored.</Item>
				</List>
			</List>
		</List>

